<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
		
	<context:annotation-config/> 
	 

	<alias alias="savethevalueFacade" name="defaultSavethevalueFacade"></alias>
	<bean id="defaultSavethevalueFacade" class="com.seqirus.facades.savethevalue.impl.SavethevalueFacadeimpl">
		<property name="savethevaluePopulator" ref="defaultSavethevaluePopulator"></property>
	</bean>
	
	
		
		<alias alias="savethevaluePopulator" name="defaultSavethevaluePopulator"></alias>
		<bean id="defaultSavethevaluePopulator" class="com.seqirus.facades.savethevalue.populator.SavethevaluePopulator"></bean>
		
</beans>
